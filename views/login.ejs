<% include ./partials/header%>
<div class="ui fixed inverted menu">
		<div class='ui container'>
			<div class="header item"><i class="stethoscope icon"></i>Home Care</div>
			<a href="/" class="item">Home</a>																									
			<a href="/Signup" class="item right item"><i class="user md icon"></i>Doctor Signup</a>
		</div>
	</div>


<div id="wrapper">
	<form name="login-form" class="login-form" action="/login" method="POST">
		<div class="header">
		<h1>Doctor Login Form</h1>
		<span>Please log In to continue</span>
		</div>
	
		<div class="content">
		<input name="username" type="text" class="input username" placeholder="Username" />
		<div class="user-icon"></div>
		<input name="password" type="password" class="input password" placeholder="Password" />
		<div class="pass-icon"></div>		
		</div>

		<div class="footer">

		<input type="submit"class="button" />
		
		</div>
	
	</form>

</div>
<div class="gradient"></div>

